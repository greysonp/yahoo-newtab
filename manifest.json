{
  "name": "Yahoo! New Tab Page",
  "manifest_version": 2,
  "version": "1.0",

  "description": "Start every new tab with Yahoo!",

  "icons": {
        "16": "img/icon.png",
        "48": "img/icon.png",
        "128": "img/icon.png"
  },

  "chrome_url_overrides": {
      "newtab": "newtab.html"
  },

  "background": {
    "scripts": [ "js/background.js" ]
  },

  "permissions": [
    "storage",
    "tabs"
  ],

  "web_accessible_resources": [
    "img/*"
  ]
}
